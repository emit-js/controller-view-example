{
  "name": "render-view-example",
  "private": true,
  "version": "0.0.18",
  "description": "",
  "keywords": [
    ""
  ],
  "author": "emit <emit@emit-js.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/emit-js/render-view-example.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/emit-js/render-view-example#readme",
  "dependencies": {
    "@emit-js/controller": "0.0.19",
    "@emit-js/el": "0.0.13",
    "@emit-js/http": "0.0.22",
    "@emit-js/log": "1.0.39",
    "@emit-js/render": "0.0.25",
    "@emit-js/store": "7.0.20",
    "@emit-js/view": "0.0.36",
    "@emit-js/emit": "3.2.9",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.4.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "husky": "1.3.1",
    "jest": "24.7.1",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "webpack-dev-server": "3.1.14"
  },
  "main": "dom.js",
  "scripts": {
    "build": "rm -rf dist; babel lib --out-dir dist --source-maps; node --experimental-modules dist/build.js",
    "prepublishOnly": "npm run build",
    "watch": "babel lib --watch --out-dir dist --source-maps",
    "fix": "npm run lint -- --fix",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "package": "rm -rf bundle; npm run build; webpack --mode=production; open bundle/index.html",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "start": "npm run build; webpack-dev-server --open"
  },
  "operations": {
    "starter": {},
    "git": {},
    "link": {},
    "spawn": {},
    "version": {},
    "watchman": {
      "triggers": [
        {
          "name": "babel",
          "expression": [
            "anyof",
            [
              "match",
              "lib/**/*.js",
              "wholename"
            ],
            [
              "match",
              "lib/**/*.jsx",
              "wholename"
            ]
          ],
          "command": [
            "npm",
            "run",
            "build"
          ]
        }
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 0.5%",
              "last 2 versions",
              "Firefox ESR",
              "not dead"
            ]
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true,
      "browser": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "curly": "error",
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "max-len": [
        "error",
        {
          "code": 60,
          "ignoreRegExpLiterals": true,
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true
        }
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "never"
      ],
      "sort-keys": [
        "error",
        "asc",
        {
          "caseSensitive": true,
          "natural": false
        }
      ],
      "sort-vars": [
        "error",
        {
          "ignoreCase": true
        }
      ],
      "strict": 0,
      "react/display-name": "off",
      "react/no-find-dom-node": "off",
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      },
      "ecmaVersion": 8,
      "sourceType": "module"
    }
  },
  "prettier": {
    "printWidth": 60,
    "semi": false,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --ignore-path .gitignore --fix",
      "git add"
    ]
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/*Test.js"
    ],
    "testURL": "http://localhost"
  }
}
